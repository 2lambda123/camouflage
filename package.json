{
  "name": "camouflage-server",
  "version": "0.1.4",
  "description": "Easily mock your webservices while testing frontends.",
  "homepage": "https://fauxauldrich.github.io/camouflage/",
  "keywords": [
    "test",
    "mock",
    "api",
    "webservice",
    "rest",
    "camouflage"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node bin/camouflage.js -m ./mocks",
    "test": "mocha tests/functional.js --reporter mochawesome --timeout 10000 --exit || true"
  },
  "bin": {
    "camouflage": "bin/camouflage.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fauxauldrich/camouflage"
  },
  "author": "Shubhendu Madhukar <shubhendumadhukar@gmail.com>",
  "license": "MIT",
  "preferGlobal": true,
  "dependencies": {
    "@grpc/grpc-js": "^1.2.12",
    "@grpc/proto-loader": "^0.6.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-winston": "^4.1.0",
    "fs-extra": "^9.1.0",
    "handlebars": "^4.7.7",
    "js-yaml": "^4.1.0",
    "jsonpath": "^1.1.1",
    "moment": "^2.29.1",
    "prom-client": "^13.0.0",
    "spdy": "^4.0.2",
    "swagger-stats": "^0.99.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.37",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "mocha": "^8.3.2",
    "mochawesome": "^6.2.2",
    "typescript": "^4.2.4"
  }
}
